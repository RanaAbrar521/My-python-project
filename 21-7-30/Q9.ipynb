   "source": [
    "def backward_number(x):\n",
    "    end=0\n",
    "    while(x>0):\n",
    "        remainder = x%10\n",
    "        end = (end*10) + remainder\n",
    "        x = x//10\n",
    "    \n",
    "    return end"
   ]
